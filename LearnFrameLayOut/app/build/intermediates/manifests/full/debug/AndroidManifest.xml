<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bryanwu95.learnframelayout"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="22" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.bryanwu95.learnframelayout.MainActivity"
            android:label="@string/app_name"
            android:theme="@android:style/Theme.NoTitleBar" >
        </activity>
        <activity
            android:name="com.bryanwu95.learnframelayout.FrameLayoutAty"
            android:label="@string/title_activity_frame_layout_aty" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.bryanwu95.learnframelayout.RelativeLayoutAty"
            android:label="@string/title_activity_relative_layout_aty" >
            android:theme="@android:style/Theme.NoTitleBar" >
        </activity>
    </application>

</manifest>